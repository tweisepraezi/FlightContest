<!-- zoom 7 -->
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="black" stroke-width="2.5" />
	</Rule>
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#613100" stroke-width="2.5" />
	</Rule>
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="black" stroke-width="1.8" />
	</Rule>


<!-- zoom 8 -->
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="black" stroke-width="3.6" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="black" stroke-width="3.2" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="black" stroke-width="2.3" />
	</Rule>


<!-- zoom 9 -->
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'motorway' or [highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="black" stroke-width="4" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="black" stroke-width="3" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'secondary'</Filter>
		<LineSymbolizer stroke="black" stroke-width="2" stroke-linecap="round" />
	</Rule>
	
<!-- zoom 10 -->
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="black" stroke-width="5" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="black" stroke-width="4.5" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="black" stroke-width="3.5" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'secondary' or [highway] = 'primary_link'</Filter>
		<LineSymbolizer stroke="black" stroke-width="2" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'tertiary' or [highway] = 'tertiary_link'</Filter>
		<LineSymbolizer stroke="black" stroke-width="1.7" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'motorway_link'</Filter>
		<LineSymbolizer stroke="black" stroke-width="4" smooth="1" />
	</Rule>
 	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'trunk_link'</Filter>
		<LineSymbolizer stroke="black" stroke-width="3" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'secondary_link'</Filter>
		<LineSymbolizer stroke="black" stroke-width="1.5" />
	</Rule>

</Style>
