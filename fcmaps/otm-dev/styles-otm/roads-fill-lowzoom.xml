<!-- zoom 7 -->
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="1.6" />
	</Rule>
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="1.6" />
	</Rule>
	<Rule>
		&maxscale_zoom7;
		&minscale_zoom7;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="white" stroke-width="1.1" />
	</Rule>


<!-- zoom 8 -->
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="2" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="1.8" />
	</Rule>
	<Rule>
		&maxscale_zoom8;
		&minscale_zoom8;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="white" stroke-width="1.4" />
	</Rule>


<!-- zoom 9 -->
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="2.3" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="2.3" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="1.6" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom9;
		&minscale_zoom9;
		<Filter>[highway] = 'secondary'</Filter>
		<LineSymbolizer stroke="white" stroke-width="1.0" stroke-linecap="round" />
	</Rule>

<!-- zoom 10 -->
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'motorway'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="3" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'trunk'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="2.8" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'motorway_link'</Filter>
		<LineSymbolizer stroke="#fdb548" stroke-width="2" smooth="1" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'trunk_link'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="2" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'primary'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="1.9" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'primary_link' or [highway] = 'secondary'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="1" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'tertiary' or [highway] = 'tertiary_link'</Filter>
		<LineSymbolizer stroke="white" stroke-width="0.8" stroke-linecap="round" />
	</Rule>
	<Rule>
		&maxscale_zoom10;
		&minscale_zoom10;
		<Filter>[highway] = 'secondary_link'</Filter>
		<LineSymbolizer stroke="#fdfd4f" stroke-width="0.7" stroke-linecap="round" />
	</Rule>

</Style>
